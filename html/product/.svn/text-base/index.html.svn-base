		<!--<script type="text/javascript" src="../../js/jquery.min-1.10.2.js"></script>-->
		<script>
			$(".button-choose").click(function() {
				$(".button-choose").css({
					"background-color": "#fafafa",
					"color": "#000000"
				});
				$(this).css({
					"background-color": "#3453a3",
					"color": "#FFFFFF"
				});

				$("#yys-web").hide();
				$("#yys-software").hide();
				$("#yxt-version").hide();

				switch(parseInt($(this).attr("tabindex"))) {
					case 0:
						$("#yys-web").show();
						break;
					case 1:
						$("#yys-software").show();
						break;
					case 2:
						$("#yxt-version").show();
						break;
				}

			});
			
			//选第二个
			$(".button-choose").eq(1).click();

			$(".img").click(function() {
				var url = $(this).attr("src");
				url = url.replace("img/", "img/big/");

				$("#my-show-img").attr("src", url);

				$("#my-show-img").css("height", (window.screen.height - 200) + "px");

				$("#my-show-img").on("load", function() {
					var imgTop = $("#my-show-img").height() / 2;
					var imgLeft = $("#my-show-img").width() / 2;
					console.log(imgLeft)
					$("#my-show-img").css({
						"margin-top": -imgTop + "px",
						"margin-left": -imgLeft + "px"
					});
				});

				$(".show-img-layer").fadeIn();
			});

			$(".dialog-mask").click(function() {
				$(".show-img-layer").fadeOut();
			});

			$("#my-show-img").click(function() {
				var url = $(this).attr("src");
				
				window.open(url);
			});
		</script>
