#about-case {
	text-align: center;
	width: 800px;
	margin: auto;
}

#about-case .pre-div {
	display: inline-block;
	margin-top: 30px;
	margin-bottom: 10px;
}

#about-case-left{
	width: 383.63px;
	height: 1103px;
}
#about-case-right{
	width: 359px;
	height: 1159px;
}

#about-case #about-case-centre {
	width: 6px;
	height: 2400px;
	background-color: #656c8a;
}

#about-case .pre-case{
	height: 215px;
}

#about-case #about-case-left .pre-case {
	text-align: left;
	font-size: 19px;
	margin-right: 50px;
}

#about-case #about-case-left .pre-case .triangle {
	width: 0px;
	height: 0px;
	border-top: 10px solid transparent;
	border-left: 20px solid #7e9874;
	border-bottom: 10px solid transparent;
	display: inline-block;
	position: relative;
	left: 100%;
	top: 70px;
}

#about-case #about-case-left .pre-case .desc {
	color: #FFFFFF;
}

#about-case #about-case-left .pre-case .desc span {
	background-color: #7e9874;
	float: right;
	padding: 12px;
	box-shadow: 1px 1px 10px #888888;
	border-radius: 2px;
	line-height: 28px;
}

#about-case #about-case-left .pre-case-color .desc span {
	background-color: #967fb5 !important;
}

#about-case #about-case-left .pre-case-color .triangle {
	border-left: 20px solid #967fb5 !important;
}

#about-case #about-case-left .pre-case-color .view span {
	color: #967fb5 !important;
}

#about-case #about-case-left .pre-case-color .view span:hover {
	color: #ab95c8 !important;
}

#about-case #about-case-left .pre-case .from {
	clear: both;
	text-align: right;
	font-size: 16px;
	padding: 20px 0px;
}

#about-case .pre-case .from span {
	font-size: 12px;
	position: relative;
	top: -3px;
	padding-right: 15px;
}

#about-case #about-case-left .pre-case .view {
