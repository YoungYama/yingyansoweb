	font-size: 16px;
	text-decoration: underline;
}

#about-case #about-case-left .pre-case .view span {
	cursor: pointer;
	color: #7e9874;
}

#about-case #about-case-left .pre-case .view span:hover {
	color: #96b28c;
}

#about-case #about-case-right .pre-case {
