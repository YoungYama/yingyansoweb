.page-box {
	margin-bottom: 20px;
	margin-top: 30px;
	line-height: 35px;
	float: left;
	clear: both;
}


.page-box li {
	list-style: none;
	display: inline-block;
}

.page-box li a {
	font-size: 14px;
	text-decoration: none;
	margin: 0 5px;
    width: 38px;
    height: 38px;
    line-height: 38px;
    background: #fff;
    border: 1px solid #ebebeb;
    color: #bdbdbd;
    padding: 10px 15px;
}

.m-style .next a,
.m-style .prev a{
    font-family: "Simsun";
    font-size: 16px;
    font-weight: bold;
    padding: 11.5px 15px;
}

.page-box li a:hover {
	background-color: #3453a3;
	color: #fff;
	border-color: #3453a3;
}

.page-box .active a {
	background-color: #3453a3;
	color: #fff;
	border-color: #3453a3;
}

.page-box .disabled {
	display: none;
}

.page-box .prev {
	border-radius: 4px 0 0px 4px;
}

.page-box .next {
	border-radius: 0px 4px 4px 0px;
}

.page-box .first,
.page-box .last {
	display: none;
}
