@charset "utf-8";
/* CSS Document */

.content{ width:1200px; height:408px; margin:0 auto; background:#f2fcff; margin-top:40px;}
.content_img{ float:left; margin-left:124px; margin-top:96px;}
.content_text{ float:left; width:480px; height:110px; margin-left:124px; margin-top:160px; letter-spacing:2px;}
.content_text p{ font-size:16px; line-height:24px;}

.title1{height:30px; text-align:center; margin-top:80px;}
.title1 p{ font-size:24px; color:#878787;}
.title1_img{height:14px; text-align:center;}
