* {
	margin: 0px;
	border: 0px;
}

header {
	background-color: #ffffff;
	border-bottom: 1px solid #c6c3c3;
	position: fixed;
	top: 0px;
	left: 0px;
	height: 80px;
	width: 100%;
	z-index: 10000;
}

#header {
	width: 1050px;
	margin: auto;
	background-color: #ffffff;
	border-bottom: 1px solid #ededed;
}

#header div {
	display: inline-block;
}

#header #logo {
	height: 55px;
	position: relative;
	top: 12px;
}

#header #header-left {
	float: left;
}

#header #header-left a {
	margin-right: 30px;
	font-size: 16px;
	padding: 35px 0px 25px 0px;
}

#header #header-left a:last-child{
	padding-right: 0px;
	margin-right: 0px;
}

#header #header-left a .icon-xiala {
	padding-left: 10px;
	font-size: 12px;
}

#header #header-profile {
	min-width: 120px;
	float: left;
	background-color: #FFFFFF;
	position: relative;
	top: 21px;
	left: 0px;
	text-align: center;
	padding: 10px 0px;
	font-size: 16px;
	border: 1px solid #c0b9b9;
	/* box-shadow: 2px 2px 3px #dfd9d9; */
	display: none;
}

#shool-profile-a{
	display: inline-block;
	width: 116px;
}

#header #header-profile p {
	padding: 5px 0px;
}

#header #header-profile a{
	padding: 0px !important;
	margin: 0px !important;
}

#header .item {
	position: relative;
	bottom: 4px;
}

#header #header-right {
	float: right;
	font-size: 16px;
	position: relative;
	top: 30px;
}

#header a {
	color: #000000;
	text-decoration: none;
}

#header .a:hover {
	text-decoration: underline;
}

#header #search-div {
	border: 1px solid #c6c3c3;
	padding: 3px;
	border-radius: 22px;
	font-size: 16px;
	background-color: #FFFFFF;
	margin-right: 20px;
}

#header #search-div #search-input {
	width: 150px;
	height: 22px;
	outline: none;
	border-radius: 22px;
	padding-left: 5px;
	border: 0px solid #000000 !important;
	text-align: left;
}

#header #search-div .icon-sousuo {
	color: #ededed;
	padding: 3px 5px 3px 0px;
	cursor: pointer;
}

#main-content {
	width: 100%;
	margin: auto;
	margin-top: 80px;
	z-index: 1;
}

footer {
	height: 100px;
	background: #3453a3;
	width: 100%;
	font-size: 13px;
	text-align: center;
	min-width: 1200px;
}

footer .foot-wz {
	display: inline-block;
	text-align: center;
	color: #fff;
	width: 450px;
	position: relative;
	left: 110px;
}

footer .foot-wz a {
	color: #fff;
	padding: 0 15px;
	text-decoration: none;
}

footer .foot-wz a:hover {
	text-decoration: underline !important;
}

footer .foot-wz .f2 {
	font-size: 13px;
	color: #fff;
	position: relative;
	top: 10px;
}

footer #qrcode {
	display: inline-block;
	width: 70px;
	height: 70px;
	background-image: url(https://www.yingyanso.cn/img/dingyue.jpg);
	background-size: 70px;
	position: relative;
	top: 15px;
	left: 145px;
}

footer #qrcode-text {
	display: inline-block;
	position: relative;
	top: 5px;
	left: 155px;
	line-height: 22px;
	color: #FFFFFF;
	font-size: 12px;
}

header #user-infos {
	font-size: 14px;
	display: none;
}

header #user-info img {
	display: inline-block;
	width: 38px;
	height: 38px;
	vertical-align: middle;
	border-radius: 50%;
	border: 1px solid #EDEDED;
	position: relative;
	bottom: 3px;
}

header #user-info .user-more {
	vertical-align: middle;
	display: inline-block;
	max-width: 100px;
}

header #user-info .user-more .user-name {
	display: inline-block;
	width: 100%;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

header #user-info .quit-box {
	padding-left: 10px;
	position: relative;
	bottom: 2px;
}

header #user-info .quit-box i {
	display: inline-block;
	vertical-align: middle;
}

header #user-info .quit-box span {
	display: inline-block;
	vertical-align: middle;
}

header #user-info .quit-box span:hover {
	text-decoration: underline;
}

header #user-info #beVIP {
	color: #FFFFFF;
	background-color: #ffdb2b;
	padding: 3px 6px;
	cursor: pointer;
}

header #user-info #beVIP:hover {
	background-color: #fee779 !important;
}

header #user-info .icon-vip {
	color: #e7e7e7;
}

header #user-info .yys-vip1 {
	color: #e7e7e7;
}

header #user-info .yys-vip2 {
	color: #ffdb2b;
}

header #user-info .yys-vip3 {
	color: #ff0303;
}

header #user-info .icon-jinbi {
	color: #ffffff;
}

header #user-info .icon-douban-copy {
	color: #eac204;
}

.yys-loading {
	position: fixed;
	z-index: 99800;
	display: none;
}

.yys-loading .dialog-mask {
	background-color: #4a4a4b;
	opacity: 0.3;
	z-index: 99900;
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0px;
	top: 0px;
}

.yys-loading .loading-box {
	z-index: 100000;
	position: fixed;
	background-color: #fff;
	left: 50%;
	top: 50%;
	padding: 30px 70px;
	margin-top: -40px;
	margin-left: -168px;
	border-radius: 2px;
	border: 1px solid #f2f2f2;
	box-shadow: 0 0 5px #f3f3f3;
	font-size: 14px;
}

.yys-loading .loading-box i {
	display: inline-block;
	animation: loading 2s infinite linear;
}

@
keyframes loading {from { -webkit-transform:rotate(0deg);
	-moz-transform: rotate(0deg);
	-ms-transform: rotate(0deg);
	-o-transform: rotate(0deg);
	transform: rotate(0deg);
}

to {
	-webkit-transform: rotate(360deg);
	-moz-transform: rotate(360deg);
	-ms-transform: rotate(360deg);
	-o-transform: rotate(360deg);
	transform: rotate(360deg);
}
}

#tip-alert-body .my-tip-mask {
	z-index: 10000;
}

#tip-alert-body .my-tip-content {
	z-index: 10000;
}
.shareYys-payLayer{
	z-index: 10000;
}

.yxt-quit{
	padding-left: 0px !important;
}

.register{
	z-index: 10000;
}

.register .sign-in .import-table .agreement a{
	text-decoration: none;
}
