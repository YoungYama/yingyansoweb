* {
	margin: 0px;
	padding: 0px;
}

a {
	text-decoration: none;
}

.yys-header {
	padding: 0px 20px;
	height: 60px;
	line-height: 60px;
	overflow: hidden;
	font-size: 12px;
	background-color: #003bb3;
}

.yys-header .link-tel {
	float: left;
	color: #fff;
}

.yys-header .product-list {
	float: right;
	color: #fff;
}

.yys-header .product-list li {
	padding: 0 10px;
	display: inline-block;
}

.yys-header .product-list li a {
	color: #fff;
}

.yys-header .product-list li a:hover {
	text-decoration: underline;
}

.yys-header .product-list .sign-btn {
	margin-left: 30px;
}

.yys-header .product-list .sign-btn a {
	margin-left: 20px;
}

.yys-header .product-list .sign-btn a i {
	display: inline-block;
	vertical-align: middle;
	margin-top: -3px;
	margin-right: 3px;
	font-size: 18px;
}

.yys-header .product-list .user-info {
	display: none;
	margin-left: 30px;
}

.yys-header .product-list .user-info img {
	display: inline-block;
	width: 30px;
	height: 30px;
	vertical-align: middle;
	border-radius: 50%;
}

.yys-header .product-list .user-info .user-more {
	vertical-align: middle;
	line-height: 16px;
	display: inline-block;
}

.yys-header .product-list .user-info .user-more .user-name {
	display: inline-block;
	width: 100%;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

.yys-header .product-list .quit-box {
	padding: 0 10px;
}

.yys-header .product-list .quit-box i {
	display: inline-block;
	vertical-align: middle;
}

.yys-header .product-list .quit-box span {
	display: inline-block;
	vertical-align: middle;
}

.yys-header .product-list .quit-box span:hover {
	text-decoration: underline;
}

#main-content-header {
	text-align: center;
}

#main-content-header #header-img {
	display: inline-block;
}

#main-content-header #header-img img {
	height: 220px;
	margin: -35px 0px 50px 0px;
}

#main-content-middle {
	background-color: #172447;
	text-align: center;
}

#main-content-middle-header {
	width: 200px;
	height: 100px;
	border-radius: 0 0 100px 100px;
	/* 左上、右上、右下、左下 */
	display: inline-block;
	background-color: #c5e8f9;
}

#middle-header-inner{
	width: 160px;
	height: 80px;
	border-radius: 0 0 80px 80px;
	/* 左上、右上、右下、左下 */
	display: inline-block;
	background-color: #FFFFFF;
}

#main-content-middle-header img {
	height: 80px;
	position: relative;
	top: -47px;
    left: 5px;
}

#main-content-middle #middle-msg {
	color: #FFFFFF;
	font-size: 40px;
	margin: 10px 0px;
	font-weight: bold;
}

#main-content-middle #middle-msg span {
	padding: 0px 20px;
	color: #ffa302;
    font-size: 50px;
}

#main-content-middle #mindle-qr img {
	width: 180px;
	margin: 20px 0px;
}

.buyButton {
	display: inline-block;
	border: 1px solid #fe5d03;
	width: 230px;
	padding: 8px 0;
	text-align: center;
	border-radius: 40px;
	color: #fff;
	font-size: 22px;
	cursor: pointer;
	background-color: #ff6100;
	margin: 10px 0px 40px 0px;
}

.buyButton:hover {
	background-color: #e65a04;
	border: 1px solid #e65a04;
}

.buyButton .arrow-right {
	display: inline-block;
	width: 40px;
	height: 40px;
	vertical-align: middle;
	background-size: contain;
	background-image: url(../img/arrow-right.png);
	background-repeat: no-repeat;
}

.buyButton span {
	display: inline-block;
	vertical-align: middle;
	margin-right: 10px;
}

#content-footer-title {
	color: #da0a0a;
	font-size: 40px;
	text-align: center;
	margin: 40px 0px 20px 0px;
	font-weight: bold;
}

#content-footer-more {
	text-align: center;
}

#content-footer-more div {
	display: inline-block;
}

#content-footer-more div img {
	height: 75px;
}

#content-footer-vips .one-footer-vip {
	width: 1150px;
	margin: auto;
	background-color: #f5e7be;
	margin-top: 20px;
	margin-bottom: 40px;
}

#content-footer-vips .footer-vip-left,
#content-footer-vips .footer-vip-right {
	display: inline-block;
}

#content-footer-vips .footer-vip-left img,
#content-footer-vips .footer-vip-right img {
	height: 122px;
}

#content-footer-vips .footer-vip-right .footer-vip-tip {
	font-size: 22px;
	position: relative;
	top: -55px;
}

#content-footer-vips .footer-vip-right .footer-vip-detail {
	font-size: 15px;
	position: relative;
	top: -28px;
	color: #9e9e9e;
}

#footer-more-get {
	text-align: center;
	margin-top: 80px;
	margin-bottom: 95px;
}

#footer-more-get #more-get-btn span {
	font-size: 23px;
	background-color: #fe5d03;
	padding: 13px 25px;
	color: #FFFFFF;
	cursor: pointer;
	border-radius: 40px;
}


#footer-more-get #more-get-btn span:hover{
	background-color: #e65a04;
}

.quick-buy1{
	padding: 10px 0;
	margin-right: 20px;
}

.quick-buy2{
	width: 260px;
	padding: 10px 0;
}

.yys-footer-2 {
    background: #052476;
}

